package zad1;

import static org.junit.Assert.*;

import org.junit.Assert;
import org.junit.Test;

public class SortowanieTekstuTest {

	@Test
	public void testGetWords() {
		SortowanieTekstu st = new SortowanieTekstu("Dupa, choj Ło ja pierdolę");
		Assert.assertEquals("Dupa, choj Ło ja pierdolę", st.getWords());
	}

}
